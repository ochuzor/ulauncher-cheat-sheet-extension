{
  "required_api_version": "^2.0.0",
  "name": "cheat-sheets",
  "description": "Demo extension",
  "developer_name": "Chinedu Eze",
  "icon": "images/icon.png",
  "options": {
    "query_debounce": 0.05
  },
  "preferences": [
    {
      "id": "demo_kw",
      "type": "keyword",
      "name": "FastTips",
      "default_value": "ft"
    },
    {
      "id": "demo_path",
      "name": "cheat sheet folder",
      "type": "input",
      "default_value": "~/cheat-sheets",
      "description": "The location of the folder containing your cheat sheet text files"
    },
    {
      "id": "item_name",
      "type": "input",
      "name": "Item name",
      "description": "Name for each item",
      "default_value": "Item"
    },
    {
      "id": "memo",
      "type": "text",
      "name": "Memo",
      "description": "Write something here",
      "default_value": ""
    }
  ]
}
